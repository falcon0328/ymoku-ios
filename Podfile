# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'Infra' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Infra
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Firestore'
  
  target 'InfraTests' do
    inherit! :search_paths
    # Pods for testing
  end

end

target 'Ymoku' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Ymoku

  target 'YmokuTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'YmokuUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
